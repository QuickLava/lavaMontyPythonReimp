<?xml version="1.0"?> 
<movesetPatch name = "PsuedoRandom Hitbox Elements and Sounds">
	<targets>
		<command name = "Offensive Collision" signature = "0x06000D00" paramIndex="12"/>
		<command name = "Special Offensive Collision" signature = "0x06150F00" paramIndex="12"/>
	</targets>
	<modifications>
		<mod match = "XXXXXXXX" lock = "FFFFFFE0">
			<action type = "0xA0" value = "00000010"/>
			<action type = "0xA4" value = "00000018"/>
		</mod>
		<mod match = "XXXXXXXX" extraCondition = "1" lock = "FFFFC1FF">
			<action type = "0xA0" value = "00000010"/>
			<action type = "0xA4" value = "0000000A"/>
			<action type = "0xA0" value = "00000001"/>
			<action type = "0xB3" value = "00000009"/>
		</mod>
	</modifications>
</movesetPatch>
