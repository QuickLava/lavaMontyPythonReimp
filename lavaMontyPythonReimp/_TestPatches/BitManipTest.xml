			<action type = "0xB0" value = "FFFFFFFF"/>
			<action type = "0xB5" value = "00000004"/>
			<action type = "0xB6" value = "00000004"/>
			<action type = "0xB3" value = "00000004"/>
			<action type = "0xB4" value = "00000004"/>
