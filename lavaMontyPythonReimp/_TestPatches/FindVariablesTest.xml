<?xml version="1.0"?> 
<movesetPatch name = "Find Variable Manips">
	<targets>
		<command name = "Basic Variable Set" 			signature = "12000200" paramIndex = "1" paramType = "5"/>
		<command name = "Basic Variable Add"			signature = "12010200" paramIndex = "1" paramType = "5"/>
		<command name = "Basic Variable Subtract"		signature = "12020200" paramIndex = "1" paramType = "5"/>
		<command name = "Basic Variable Multiply"		signature = "120D0200" paramIndex = "1" paramType = "5"/>
		<command name = "Basic Variable Divide"			signature = "120E0200" paramIndex = "1" paramType = "5"/>
		<command name = "Basic Variable Increment"		signature = "12030200" paramIndex = "0" paramType = "5"/>
		<command name = "Basic Variable Decrement"		signature = "12040200" paramIndex = "0" paramType = "5"/>
		<command name = "Basic Variable Random"			signature = "12050300" paramIndex = "2" paramType = "5"/>
		<command name = "Basic Variable Set Absolute"	signature = "12110200" paramIndex = "1" paramType = "5"/>
		<command name = "Float Variable Set" 			signature = "12060200" paramIndex = "1" paramType = "5"/>
		<command name = "Float Variable Add"			signature = "12070200" paramIndex = "1" paramType = "5"/>
		<command name = "Float Variable Subtract"		signature = "12080200" paramIndex = "1" paramType = "5"/>
		<command name = "Float Variable Multiply"		signature = "120F0200" paramIndex = "1" paramType = "5"/>
		<command name = "Float Variable Divide"			signature = "12100200" paramIndex = "1" paramType = "5"/>
		<command name = "Float Variable Random"			signature = "12090300" paramIndex = "2" paramType = "5"/>
		<command name = "Float Variable Set Absolute"	signature = "12120200" paramIndex = "1" paramType = "5"/>
		<command name = "Bit Variable Set"				signature = "120A0200" paramIndex = "1" paramType = "5"/>
		<command name = "Bit Variable Clear"			signature = "120B0200" paramIndex = "1" paramType = "5"/>
		<command name = "If (1 Var)"					signature = "000A0200" paramIndex = "1" paramType = "5"/>
	</targets>
	<modifications>
		<mod name = "IC-Basic"	match = "00XXXXXX">
		</mod>
		<mod name = "IC-Float"	match = "01XXXXXX">
		</mod>
		<mod name = "IC-Bit"	match = "02XXXXXX">
		</mod>
		<mod name = "LA-Basic"	match = "10XXXXXX">
		</mod>
		<mod name = "LA-Float"	match = "11XXXXXX">
		</mod>
		<mod name = "LA-Bit"	match = "12XXXXXX">
		</mod>
		<mod name = "RA-Basic"	match = "20XXXXXX">
		</mod>
		<mod name = "RA-Float"	match = "21XXXXXX">
		</mod>
		<mod name = "RA-Bit"	match = "22XXXXXX">
		</mod>
	</modifications>
</movesetPatch>

<movesetPatch name = "Find Variable Manips (2 Var If Statements)">
	<targets>
		<command name = "If (2 Var)"	signature = "0x000A0400" paramIndex="1" paramType = "5"/>
	</targets>
	<modifications>
		<mod name = "[Var 1] IC-Basic"	match = "00XXXXXX">
		</mod>
		<mod name = "[Var 1] IC-Float"	match = "01XXXXXX">
		</mod>
		<mod name = "[Var 1] IC-Bit"	match = "02XXXXXX">
		</mod>
		<mod name = "[Var 1] LA-Basic"	match = "10XXXXXX">
		</mod>
		<mod name = "[Var 1] LA-Float"	match = "11XXXXXX">
		</mod>
		<mod name = "[Var 1] LA-Bit"	match = "12XXXXXX">
		</mod>
		<mod name = "[Var 1] RA-Basic"	match = "20XXXXXX">
		</mod>
		<mod name = "[Var 1] RA-Float"	match = "21XXXXXX">
		</mod>
		<mod name = "[Var 1] RA-Bit"	match = "22XXXXXX">
		</mod>
		<mod name = "[Var 2] IC-Basic"	match = "00XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] IC-Float"	match = "01XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] IC-Bit"	match = "02XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] LA-Basic"	match = "10XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] LA-Float"	match = "11XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] LA-Bit"	match = "12XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] RA-Basic"	match = "20XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] RA-Float"	match = "21XXXXXX" paramIndexRedirect = "3">
		</mod>
		<mod name = "[Var 2] RA-Bit"	match = "22XXXXXX" paramIndexRedirect = "3">
		</mod>
	</modifications>
</movesetPatch>
